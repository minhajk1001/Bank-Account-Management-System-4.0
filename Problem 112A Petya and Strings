#include <iostream>
#include <sstream>
using namespace std;

int main() {
	
	string s,ss,test="abcdefghijklmnopqrstuvwxyz";
	cin>>s>>ss;
	int counts=0,countss=0;
	
	for(int i=0; i<=s.size(); i++){
		s[i]=tolower(s[i]);
		ss[i]=tolower(ss[i]);
	}
	
	for(int i=0; i<s.size(); i++){
		
			if(s[i]!=ss[i]){
				
				for(int k=0; k<test.size(); k++){
					if(s[i]!=test[k])
						counts++;
					else
						break;
					}
					
				for(int k=0; k<test.size(); k++){
					if(ss[i]!=test[k])
						countss++;
					else
						break;
					}
					
				if(counts<countss){
					cout<<"-1"<<endl;
					return 0;
				}
				if(counts>countss){
					cout<<"1"<<endl;
					return 0;
				}
			}
			if(s[i]=ss[i])
				counts++;
			if(counts==s.size()){
				cout<<"0"<<endl;
				return 0;
			}
		}
	}
